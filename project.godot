config_version=4

[application]

config/name="Go Godot 3 Classic - ClownPanic"
run/main_scene="res://MainScene.tscn"
config/icon="res://Images/icon.png"

[display]

window/size/width=1920
window/size/height=1080

[editor_plugins]

enabled=PoolStringArray( "res://addons/WinXP-Window/plugin.cfg" )

[input]

enable_mouse={
